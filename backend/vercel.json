{
  "builds": [
    {
      "config": {
        "excludeFiles": [
          "logs/**",
          "*.log"
        ],
        "includeFiles": [
          "node_modules/**",
          "package.json",
          "package-lock.json",
          "config/**",
          "models/**",
          "routes/**",
          "middlewares/**"
        ]
      },
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "dest": "index.js",
      "src": "/api/(.*)"
    },
    {
      "dest": "index.js",
      "src": "/(.*)"
    }
  ],
  "version": 2
}
