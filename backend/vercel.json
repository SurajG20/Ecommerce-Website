{
  "builds": [
    {
      "config": {
        "includeFiles": [
          "node_modules/**",
          "package.json",
          "package-lock.json"
        ]
      },
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "dest": "index.js",
      "src": "/api/(.*)"
    },
    {
      "dest": "index.js",
      "src": "/(.*)"
    }
  ],
  "version": 2
}
